proc            /proc           proc    defaults          0       0
BOOTDEV  /boot           vfat    defaults          0       2
ROOTDEV  /               ext4    defaults,noatime  0       1

# Add tmpfs at /tmp to reduce EMMC wear
tmpfs /var/tmp tmpfs defaults,noatime,nosuid,nodev,noexec,mode=0755,size=64M 0 0